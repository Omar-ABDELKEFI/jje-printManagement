<web-app xmlns="http://java.sun.com/xml/ns/j2ee" version="2.4"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http:/java.sun.com/dtd/web-app_2_3.dtd">
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>mjjs.controller.HelloServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>

   <servlet>
    <description></description>
    <display-name>AgentDashboardServlet</display-name>
    <servlet-name>AgentDashboardServlet</servlet-name>
    <servlet-class>mjjs.controller.AgentDashboardServlet</servlet-class>
    <url-pattern>/AgentDashboard</url-pattern>
  </servlet>
  <servlet-mapping>
    <servlet-name>AgentDashboardServlet</servlet-name>
    <url-pattern>/AgentDashboard</url-pattern>
  </servlet-mapping>
</web-app>
