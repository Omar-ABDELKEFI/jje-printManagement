<web-app xmlns="http://java.sun.com/xml/ns/j2ee" version="2.4"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http:/java.sun.com/dtd/web-app_2_3.dtd">
    <servlet>
        <servlet-name>hello</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.HelloServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>hello</servlet-name>
        <url-pattern>/hello</url-pattern>
    </servlet-mapping>

    <!-- Define PrintRequestServlet -->
    <servlet>
        <servlet-name>PrintRequestServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.PrintRequestServlet</servlet-class>
        <multipart-config>
            <max-file-size>20848820</max-file-size>            <!-- Max size in bytes (20MB) -->
            <max-request-size>418018841</max-request-size>            <!-- Max size of multipart/form-data request in bytes (50MB) -->
            <file-size-threshold>1048576</file-size-threshold>            <!-- Size threshold for buffering files in bytes (1MB) -->
        </multipart-config>
    </servlet>

    <!-- Map PrintRequestServlet to /printRequest -->
    <servlet-mapping>
        <servlet-name>PrintRequestServlet</servlet-name>
        <url-pattern>/printRequest</url-pattern>
    </servlet-mapping>

    <!-- Define LoginServlet -->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.LoginServlet</servlet-class>
    </servlet>

    <!-- Map LoginServlet to /LoginServlet -->
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <!-- Define LoginServlet -->
    <servlet>
        <servlet-name>EditUserServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.EditUserServlet</servlet-class>
    </servlet>

    <!-- Map LoginServlet to /LoginServlet -->
    <servlet-mapping>
        <servlet-name>EditUserServlet</servlet-name>
        <url-pattern>/EditUserServlet</url-pattern>
    </servlet-mapping>

    <!-- Define AdminDashboardServlet -->
    <servlet>
        <servlet-name>AdminDashboardServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.AdminDashboardServlet</servlet-class>
    </servlet>

    <!-- Map LoginServlet to /AdminDashboardServlet -->
    <servlet-mapping>
        <servlet-name>AdminDashboardServlet</servlet-name>
        <url-pattern>/AdminDashboardServlet</url-pattern>
    </servlet-mapping>

    <!-- Define UpdateStatusServlet -->
    <servlet>
        <servlet-name>UpdateStatusServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.UpdateStatusServlet</servlet-class>
    </servlet>

    <!-- Map PrintRequestServlet to /printRequest -->
    <servlet-mapping>
        <servlet-name>UpdateStatusServlet</servlet-name>
        <url-pattern>/UpdateStatusServlet</url-pattern>
    </servlet-mapping>

    <!-- Define AddUserServlet -->
    <servlet>
        <servlet-name>AddUserServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.AddUserServlet</servlet-class>
    </servlet>

    <!-- Map AddUserServlet to /AddUserServlet -->
    <servlet-mapping>
        <servlet-name>AddUserServlet</servlet-name>
        <url-pattern>/AddUserServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetMatiersServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.GetMatiersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetMatiersServlet</servlet-name>
        <url-pattern>/getMatiers</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>AgentDashboardServlet</display-name>
        <servlet-name>AgentDashboardServlet</servlet-name>
        <servlet-class>com.example.jee.printing.platform.controller.AgentDashboardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AgentDashboardServlet</servlet-name>
        <url-pattern>/agentDashboard</url-pattern>
    </servlet-mapping>
</web-app>
